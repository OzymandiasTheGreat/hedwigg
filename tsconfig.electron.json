{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "lib": [
      "es2019",
      "es2018",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "src/main.electron.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
